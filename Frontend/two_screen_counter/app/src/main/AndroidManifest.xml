<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.as1">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AS1">
        <activity
            android:name=".screens.HomePage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".screens.StockPage"
            android:exported="true" />
        <activity
            android:name=".screens.ProfilePage"
            android:exported="true" />
        <activity
            android:name=".screens.MainPage"
            android:exported="true" />
        <activity
            android:name=".screens.CreateAccountPage"
            android:exported="true" />
        <activity
            android:name=".screens.LoginPage"
            android:exported="true" />

    </application>

</manifest>